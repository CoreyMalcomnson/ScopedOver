				WorldViewDirection = SafeNormalize( WorldViewDirection );

				float4 triplanar21 = TriplanarSamplingSF( _GrassTexture, WorldPosition, WorldNormal, 1.0, _GrassTextureSize, 1.0, 0 );
				float4 triplanar31 = TriplanarSamplingSF( _MudTexture, WorldPosition, WorldNormal, 1.0, _MudTextureSize, 1.0, 0 );
				float4 lerpResult5 = lerp( triplanar21 , triplanar31 , IN.ase_color.g);
				float4 triplanar43 = TriplanarSamplingSF( _CliffTexture, WorldPosition, WorldNormal, 1.0, _CliffTextureSize, 1.0, 0 );
				float4 lerpResult44 = lerp( lerpResult5 , triplanar43 , IN.ase_color.b);
				float4 triplanar55 = TriplanarSamplingSF( _StoneTexture, WorldPosition, WorldNormal, 1.0, _StoneTextureSize, 1.0, 0 );
				float4 lerpResult48 = lerp( lerpResult44 , triplanar55 , ( 1.0 - IN.ase_color.a ));
				float4 Terrain_Albedo59 = lerpResult48;
				
				float3x3 ase_worldToTangent = float3x3(WorldTangent,WorldBiTangent,WorldNormal);
				float3 triplanar106 = TriplanarSamplingSNF( _GrassNormal, WorldPosition, WorldNormal, 1.0, _GrassNormalSize, 1.0, 0 );
